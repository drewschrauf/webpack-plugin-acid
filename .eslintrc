{
    "parser": "babel-eslint",
    "extends": ["defaults"],
    "ecmaFeatures": {
        "modules": true,
        "arrowFunctions": true,
        "blockBindings": true,
        "templateStrings": true,
        "objectLiteralShorthandProperties": true
    },
    "rules": {
        "semi": 2,
        "eqeqeq": 2
    },
    "env": {
        "node": true,
        "mocha": true
    }
}
